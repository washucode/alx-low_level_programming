#include "lists.h"

/**
 * list_len - return len of linked list
 * @list_t : linked list
 * @h : pointer to linkedlist
 * Return: number fo elements on linked list
 */

size_t list_len(const list_t *h)
{
	size_t elements = 0;
	while (h != NULL)
	{
		h = h->next;
		elements++;
	}
	return (elements);
}
